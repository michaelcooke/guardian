{
    "name": "michaelcooke/guardian",
    "description": "A simple permissions and roles package for Laravel",
    "keywords": ["laravel", "laravel-5", "permissions", "restrictions", "roles"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Cooke",
            "email": "michael.frank.cooke@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "MichaelCooke\\Guardian\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelCooke\\Guardian\\GuardianServiceProvider"
            ],
            "aliases": {
                "Permission": "MichaelCooke\\Guardian\\Facades\\Permission::class",
                "Role": "MichaelCooke\\Guardian\\Facades\\Role::class"
            }
        }
    }
}
